---
import { getByUID } from "@/components/core/prismic";
import HomeServiceComp from ".";

const { data } = Astro.props;

const listServicesUID = await data.items.map(item => item.service.uid);
const listService = await Promise.all(
    listServicesUID.map(async uid => {
        const apiData = await getByUID('service', uid);
        return apiData;
    })
);
---

<HomeServiceComp Expertise={listService} {...data} client:visible={{rootMargin: "100% 0%"}}/>