---
const { title } = Astro.props;

import { getPage } from "@/components/core/prismic";

const { data } = await getPage("global_info");

---
<head>
    <meta charset="UTF-8" />
    <meta name="description" content="We the brand" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <meta name="generator" content={Astro.generator} />
    <link rel="icon" type="image/svg+xml" href={data.favicon.url} />
    <title>{title}</title>
</head>